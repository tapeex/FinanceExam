        <SolidColorBrush x:Key="Button.Static.Background4" Color="DarkGray"/>
        <SolidColorBrush x:Key="Button.Static.Border4" Color="DarkGray"/>
        <SolidColorBrush x:Key="Button.MouseOver.Background4" Color="DarkGray"/>
        <SolidColorBrush x:Key="Button.MouseOver.Border4" Color="DarkGray"/>
        <SolidColorBrush x:Key="Button.Pressed.Background4" Color="DarkGray"/>
        <SolidColorBrush x:Key="Button.Pressed.Border4" Color="DarkGray"/>
        <SolidColorBrush x:Key="Button.Disabled.Background4" Color="DarkGray"/>
