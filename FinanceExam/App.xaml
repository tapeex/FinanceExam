        <SolidColorBrush x:Key="Button.Static.Background1" Color="PaleVioletRed"/>
        <SolidColorBrush x:Key="Button.Static.Border1" Color="PaleVioletRed"/>
        <SolidColorBrush x:Key="Button.MouseOver.Background1" Color="PaleVioletRed"/>
        <SolidColorBrush x:Key="Button.MouseOver.Border1" Color="PaleVioletRed"/>
        <SolidColorBrush x:Key="Button.Pressed.Background1" Color="PaleVioletRed"/>
        <SolidColorBrush x:Key="Button.Pressed.Border1" Color="PaleVioletRed"/>
        <SolidColorBrush x:Key="Button.Disabled.Background1" Color="PaleVioletRed"/>
